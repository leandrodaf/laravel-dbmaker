{
    "name": "dbmaker/laravel-odbc",
    "version": "2.0.2",
    "description": "DBMaker integration for Laravel framework ",
    "keywords": [
        "laravel",
        "DBMaker",
        "Database",
        "PDO",
        "ODBC"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "http://www.dbmaker.com.tw",
    "authors": [
        {
            "name": "syscom",
            "email": "info@syscom.com.tw",
            "homepage": "http://www.dbmaker.com.tw",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "illuminate/database": "*"
    },
    "autoload": {
        "psr-4": {
            "DBMaker\\ODBC\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DBMaker\\ODBC\\DBMakerServiceProvider"
            ]
        }
    }
}
